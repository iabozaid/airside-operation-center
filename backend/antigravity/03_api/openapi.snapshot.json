  "components": {
    "schemas": {
      "AssetCreateRequest": {
        "properties": {
          "asset_type": {
            "type": "string",
            "title": "Asset Type"
          },
          "name": {
            "type": "string",
            "title": "Name"
          }
        },
        "type": "object",
        "required": [
          "asset_type",
          "name"
        ],
        "title": "AssetCreateRequest"
      },
      "AssetResponse": {
        "properties": {
          "id": {
            "type": "string",
            "title": "Id"
          },
          "asset_type": {
            "type": "string",
            "title": "Asset Type"
          },
          "name": {
            "type": "string",
            "title": "Name"
          },
          "status": {
            "type": "string",
            "title": "Status"
          },
          "last_heartbeat_utc": {
            "anyOf": [
              {
                "type": "string",
                "format": "date-time"
              },
              {
                "type": "null"
              }
            ],
            "title": "Last Heartbeat Utc"
          }
        },
        "type": "object",
        "required": [
          "id",
          "asset_type",
          "name",
          "status"
        ],
        "title": "AssetResponse"
      },
      "AssetStatusUpdateRequest": {
        "properties": {
          "status": {
            "type": "string",
            "title": "Status"
          }
        },
        "type": "object",
        "required": [
          "status"
        ],
        "title": "AssetStatusUpdateRequest"
      },
      "AssetStatusUpdateResponse": {
        "properties": {
          "status": {
            "type": "string",
            "title": "Status"
          },
          "updated_at_utc": {
            "type": "string",
            "format": "date-time",
            "title": "Updated At Utc"
          }
        },
        "type": "object",
        "required": [
          "status",
          "updated_at_utc"
        ],
        "title": "AssetStatusUpdateResponse"
      },
      "BaggageStatsResponse": {
        "properties": {
          "timestamp_utc": {
            "type": "string",
            "format": "date-time",
            "title": "Timestamp Utc"
          },
          "throughput": {
            "$ref": "#/components/schemas/BaggageThroughput"
          },
          "health": {
            "$ref": "#/components/schemas/HealthStatus"
          }
        },
        "type": "object",
        "required": [
          "timestamp_utc",
          "throughput",
          "health"
        ],
        "title": "BaggageStatsResponse"
      },
      "BaggageThroughput": {
        "properties": {
          "bags_per_min": {
            "type": "number",
            "minimum": 0.0,
            "title": "Bags Per Min"
          },
          "bags_last_15_min": {
            "type": "integer",
            "minimum": 0.0,
            "title": "Bags Last 15 Min"
          },
          "bags_today": {
            "type": "integer",
            "minimum": 0.0,
            "title": "Bags Today"
          }
        },
        "type": "object",
        "required": [
          "bags_per_min",
          "bags_last_15_min",
          "bags_today"
        ],
        "title": "BaggageThroughput"
      },
      "CreateUserRequest": {
        "properties": {
          "username": {
            "type": "string",
            "title": "Username"
          },
          "password": {
            "type": "string",
            "title": "Password"
          },
          "roles": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "title": "Roles"
          }
        },
        "type": "object",
        "required": [
          "username",
          "password",
          "roles"
        ],
        "title": "CreateUserRequest"
      },
      "EscalationResponse": {
        "properties": {
          "status": {
            "type": "string",
            "title": "Status"
          },
          "incident_id": {
            "type": "string",
            "title": "Incident Id"
          },
          "ticket_id": {
            "type": "string",
            "title": "Ticket Id"
          },
          "ticket_status": {
            "type": "string",
            "title": "Ticket Status"
          }
        },
        "type": "object",
        "required": [
          "status",
          "incident_id",
          "ticket_id",
          "ticket_status"
        ],
        "title": "EscalationResponse"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "HealthStatus": {
        "properties": {
          "status": {
            "$ref": "#/components/schemas/HealthStatusEnum"
          },
          "note": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Note"
          }
        },
        "type": "object",
        "required": [
          "status"
        ],
        "title": "HealthStatus"
      },
      "HealthStatusEnum": {
        "type": "string",
        "enum": [
          "mock",
          "ok",
          "degraded",
          "down"
        ],
        "title": "HealthStatusEnum"
      },
      "IncidentResponse": {
        "properties": {
          "id": {
            "type": "string",
            "title": "Id"
          },
          "type": {
            "type": "string",
            "title": "Type"
          },
          "severity": {
            "type": "string",
            "title": "Severity"
          },
